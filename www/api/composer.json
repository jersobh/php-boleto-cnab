{
    "name": "jersobh/php-boleto-cnab",
    "description": "API's para boletos com registro (CNAB 240/CNAB 400), para gerar boletos, arquivos de remessa, processar arquivos de retorno e integrações com webservices.",
    "type": "project",
    "keywords": ["slim-framework", "API", "CNAB", "BOLETO"],
    "authors": [
        {
            "name": "Jefferson de Andrade Santos",
            "email": "jersobh@gmail.com",
            "homepage": "http://dataforge.com.br"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": [
              "app/src",
              "tests/app/src"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*",
        "squizlabs/php_codesniffer": "2.*"
    },
    "require": {
        "slim/slim": "^3.0",
        "slim/twig-view": "^2.0",
        "slim/flash": "^0.1.0",
        "monolog/monolog": "^1.13",
        "rctnet/opencnabphp": "@dev",
        "tiexpert/ws-boleto-santander": "@dev",
        "andersondanilo/cnab_php": "@dev",
        "cobregratis/boletophp": "dev-2.x-dev"
    },
    "minimum-stability": "dev"
}
